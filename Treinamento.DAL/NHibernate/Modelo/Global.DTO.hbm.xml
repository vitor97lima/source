<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Treinamento.DTO" namespace="Treinamento.DTO.Global" schema="glo">
  <class name="UnidadeFederativa" table="tbUnidadeFederativa">
    <id name="Id" column="ufe_int_id">
      <generator class="native">
        <param name="sequence">sqUnidadeFederativaId</param>
      </generator>
    </id>
    <property name="Nome" column="ufe_str_nome" not-null="true" length="255"/>
    <property name="Sigla" column="ufe_str_sigla" not-null="true" length="50"/>
  </class>
  </hibernate-mapping>

